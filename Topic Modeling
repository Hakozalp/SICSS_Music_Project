## Use TidyText Approaches - 1 line per word

library(tidytext)
library(stopwords)

head(stopwords::stopwords("tr", source = "nltk"), 500)

tidy_turkish_lyrics <- turkish_lyrics_corpus %>%
  mutate(line = row_number()) %>%
  unnest_tokens(word, lyric) %>%
  anti_join(stop_words) %>%
  filter(word!="acaba",) %>%
  filter(word!="ama") %>%
  filter(word!="aslında") %>%
  filter(word!="az") %>%
  filter(word!="bazı") %>%
  filter(word!="belki") %>%
  filter(word!="biri") %>%
  filter(word!="birkaç") %>%
  filter(word!="birşey") %>%
  filter(word!="biz") %>%
  filter(word!="bu") %>%
  filter(word!="çok") %>%
  filter(word!="çünkü") %>%
  filter(word!="da") %>%
  filter(word!="daha") %>%
  filter(word!="de") %>%
  filter(word!="defa") %>%
  filter(word!="diye") %>%
  filter(word!="eğer") %>%
  filter(word!="en") %>%
  filter(word!="gibi") %>%
  filter(word!="hem") %>%
  filter(word!="hep") %>%
  filter(word!="hepsi") %>%
  filter(word!="her") %>%
  filter(word!="için") %>%
  filter(word!="ile") %>%
  filter(word!="ise") %>%
  filter(word!="kez") %>%
  filter(word!="ne") %>%
  filter(word!="neden") %>%
  filter(word!="nerde") %>%
  filter(word!="nerede") %>%
  filter(word!="nasıl") %>%
  filter(word!="veya") %>%
  filter(word!="o") %>%
  filter(word!="ve") %>%
  filter(word!="ya") %>%
  filter(word!="yani") %>%
  filter(word!="bir") %>%
  filter(word!="yeah") %>%
  filter(word!="yok") %>%
  filter(word!="ah") %>%
  filter(word!="bana") %>%
  filter(word!="sen") %>%
  filter(word!="ben") %>%
  filter(word!="bi") %>%
  filter(word!="var") %>%
  filter(word!="beni") %>%
  filter(word!="ey") %>%
  filter(word!="yine") %>%
  filter(word!="ki") %>%
  filter(word!="hiç") %>%
  filter(word!="değil") %>%
  filter(word!="ja") %>%
  filter(word!="tek") %>%
  filter(word!="niye") %>%
  filter(word!="mm") %>%
  filter(word!="yummy") %>%
  filter(word!="ooh") %>%
  filter(word!="ayy") %>%
  filter(word!="na") %>%
  filter(word!="doo") %>%
  filter(word!="pika") %>%
  filter(word!="bak") %>%
  filter(word!="sana") %>%
  filter(word!="kim") %>%
  filter(word!="sık") %>%
  filter(word!="artık") %>%
  filter(word!="yeniden") %>%
  filter(word!="seni") %>%
  filter(word!="benim") %>%
  filter(word!="weyo") %>%
  filter(word!="bile") %>%
  filter(word!="ha") %>%
  filter(word!="fazla") %>%
  filter(word!="andale") %>%
  filter(word!="skrrrt") %>%
  filter(word!="skrrt") %>%
  filter(word!="çu")

tidy_turkish_lyrics %>%
  count(word, sort = TRUE)

library(quanteda)
library(stm)

turkish_lyrics_dfm <- tidy_turkish_lyrics %>%
  count(track_title, word, sort = TRUE) %>%
  cast_dfm(track_title, word, n)

turkish_lyrics_sparse <- tidy_turkish_lyrics %>%
  count(track_title, word, sort = TRUE) %>%
  cast_sparse(track_title, word, n)

topic_model_turkish_lyrics <- stm(turkish_lyrics_dfm, K = 10, 
                   verbose = FALSE, init.type = "Spectral")

td_beta_turkish_lyrics <- tidy(topic_model_turkish_lyrics)

td_beta_turkish_lyrics %>%
  group_by(topic) %>%
  top_n(10, beta) %>%
  ungroup() %>%
  mutate(topic = paste0("Topic ", topic),
         term = reorder_within(term, beta, topic)) %>%
  ggplot(aes(term, beta, fill = as.factor(topic))) +
  geom_col(alpha = 0.8, show.legend = FALSE) +
  facet_wrap(~ topic, scales = "free_y") +
  coord_flip() +
  scale_x_reordered() +
  labs(x = NULL, y = expression(beta),
       title = "Highest word probabilities for each topic",
       subtitle = "Different words are associated with different topics")

