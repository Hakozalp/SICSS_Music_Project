try(uk_lyrics_corpus  <-rbind(uk_lyrics_corpus,song_lyrics_uk_1830))

tidy_uk_lyrics <- uk_lyrics_corpus %>%
  mutate(line = row_number()) %>%
  unnest_tokens(word, lyric) %>%
  anti_join(stop_words) %>%
  filter(word!="yeah") %>%
  filter(word!="ooh") %>%
  filter(word!="la") %>%
  filter(word!="wanna") %>%
  filter(word!="da") %>%
  filter(word!="uh") %>%
  filter(word!="ah") %>%
  filter(word!="mmm") %>%
  filter(word!="mmmh") %>%
  filter(word!="opp") %>%
  filter(word!="ayy") %>%
  filter(word!="eh") %>%
  filter(word!="ddu") %>%
  filter(word!="na") %>%
  filter(word!="doo") %>%
  filter(word!="woo") %>%
  filter(word!="mi") %>%
  filter(word!="   ") %>%
  filter(word!="□□□")

tidy_uk_lyrics %>%
  count(word, sort = TRUE)

uk_lyrics_dfm <- tidy_uk_lyrics %>%
  count(track_title, word, sort = TRUE) %>%
  cast_dfm(track_title, word, n)

uk_lyrics_sparse <- tidy_uk_lyrics %>%
  count(track_title, word, sort = TRUE) %>%
  cast_sparse(track_title, word, n)

topic_model_uk_lyrics <- stm(uk_lyrics_dfm, K = 8, 
                                  verbose = FALSE, init.type = "Spectral")

td_beta_uk_lyrics <- tidy(topic_model_uk_lyrics)

td_beta_uk_lyrics %>%
  group_by(topic) %>%
  top_n(10, beta) %>%
  ungroup() %>%
  mutate(topic = paste0("Topic ", topic),
         term = reorder_within(term, beta, topic)) %>%
  ggplot(aes(term, beta, fill = as.factor(topic))) +
  geom_col(alpha = 0.8, show.legend = FALSE) +
  facet_wrap(~ topic, scales = "free_y") +
  coord_flip() +
  scale_x_reordered() +
  labs(x = NULL, y = expression(beta),
       title = "Highest word probabilities for each topic",
       subtitle = "Different words are associated with different topics")

td_gamma_turkish_lyrics <- tidy(topic_model_turkish_lyrics, matrix = "gamma",                    
                 document_names = rownames(turkish_lyrics_dfm))

td_gamma_turkish_lyrics<-data.table::as.data.table(td_gamma_turkish_lyrics)

td_gamma_turkish_lyrics_final<- td_gamma_turkish_lyrics %>%
  group_by(document)%>%
  arrange(-gamma)

td_gamma_turkish_lyrics_final<- td_gamma_turkish_lyrics_final%>%
  distinct(document, .keep_all=TRUE)

Final_merged_turkey_data<-merge(merged_turkey_data,td_gamma_turkish_lyrics_final,by.x="Track.Name",by.y ="document")

td_gamma_uk_lyrics <- tidy(topic_model_uk_lyrics, matrix = "gamma",                    
                                document_names = rownames(uk_lyrics_dfm))

td_gamma_uk_lyrics<-data.table::as.data.table(td_gamma_uk_lyrics)

td_gamma_uk_lyrics_final<- td_gamma_uk_lyrics %>%
  group_by(document)%>%
  arrange(-gamma)

td_gamma_uk_lyrics_final<- td_gamma_uk_lyrics_final%>%
  distinct(document, .keep_all=TRUE)

Final_merged_uk_data<-merge(merged_uk_data,td_gamma_uk_lyrics_final,by.x="Track.Name",by.y ="document")
